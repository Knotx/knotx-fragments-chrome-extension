{
  "name": "knotx-chrome-devtool-extension",
  "version": "1.0.0",
  "description": "Extends the Developer Tools, adding a sidebar that displays the Knotx Fragments data associated with the selected DOM element.",
  "private": true,
  "browserslist": [
    "last 1 chrome version"
  ],
  "scripts": {
    "dev": "webpack",
    "watch": "webpack --watch",
    "test": "jest -c jest.config.js",
    "lint": "eslint -c .eslintrc.json 'src/js/**'",
    "release": "node ./scripts/build.js",
    "release:minor": "node ./scripts/build.js minor",
    "release:major": "node ./scripts/build.js major",
    "clean:artifacts": "rm -rf packages/*.zip"
  },
  "keywords": [
    "knot.x",
    "debug",
    "chrome plugin"
  ],
  "author": "Knot.x community",
  "license": "Apache License 2.0",
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@testing-library/jest-dom": "^5.1.1",
    "archiver": "^3.1.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.7",
    "canvas": "^2.6.1",
    "css-loader": "^3.4.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-header": "^3.0.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "jest-extended": "^0.11.5",
    "jest-junit-reporter": "^1.1.0",
    "semver": "^7.1.2",
    "style-loader": "^1.1.3",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.5",
    "renderjson": "^1.4.0",
    "styled-components": "^5.0.1",
    "unique-selector": "^0.4.1",
    "vis-network": "^7.3.4",
    "vis-timeline": "^7.1.2",
    "webext-redux": "^2.1.6"
  }
}
